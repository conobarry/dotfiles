{
    "editor.minimap.enabled": false,
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
    "workbench.colorTheme": "Default Light+",
    "workbench.colorCustomizations": {
        "[Default Light+]": {
            "editor.background": "#f5efec",
            "editorGroupHeader.tabsBackground": "#EDE7E3",
            "tab.inactiveBackground": "#EDE7E3",
            "panel.background": "#f5efec",
            "sideBar.background": "#EDE7E3",
        }
    },
    "[lua]": {
        "editor.tabSize": 2
    },
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "Lua.workspace.library": {
        "/usr/share/awesome": true
    },
    "Lua.workspace.maxPreload": 1000,
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    // "outline.showProperties": false,
    // "outline.showStrings": false,
    // "outline.showStructs": false,
    // "outline.showConstants": false,
    // "outline.showVariables": false,
    // "outline.showArrays": false,
    // "outline.showBooleans": false,
    // "outline.showFunctions": false,
    // "outline.showNull": false,
    // "outline.showNumbers": false,
    // "outline.showObjects": false,
    // "outline.showOperators": false,
    // "outline.showFields": false,
    "editor.semanticHighlighting.enabled": true,
    "latex-workshop.view.pdf.viewer": "tab",
    "latex.server.autoDownload": true,
    "latex-workshop.chktex.enabled": true,
    "explorer.openEditors.sortOrder": "alphabetical",
    "latex-workshop.message.warning.show": false,
    "latex-workshop.latex.outDir": "%DIR%/out",
    "Lua.telemetry.enable": false,
    "window.enableMenuBarMnemonics": false,
    "window.customMenuBarAltFocus": false,
    "window.menuBarVisibility": "hidden",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "editor.trimAutoWhitespace": false,
    "workbench.editor.enablePreviewFromCodeNavigation": true,
    "debug.openDebug": "neverOpen",
    "window.restoreWindows": "none",
    "terminal.integrated.fontFamily": "RobotoMono Nerd Font",
    "editor.fontSize": 12,
    "security.workspace.trust.untrustedFiles": "open",
    "workbench.startupEditor": "none"
}